{
  "name": "sails-hook-sockets",
  "version": "1.4.3",
  "description": "Implements socket.io support in Sails",
  "keywords": [
    "socket.io",
    "sails.js",
    "sails-hook",
    "hook"
  ],
  "dependencies": {
    "@sailshq/lodash": "^3.10.2",
    "async": "2.0.1",
    "flaverr": "^1.0.0",
    "machinepack-redis": "^1.1.1",
    "machinepack-urls": "^3.1.1",
    "semver": "4.3.6",
    "socket.io": "1.7.3",
    "uid2": "0.0.3"
  },
  "devDependencies": {
    "ioredis": "2.4.0",
    "machinepack-fs": "^3.0.0",
    "machinepack-http": "^2.1.2",
    "mocha": "3.1.0",
    "redis": "2.6.3",
    "sails": "^1.0.0-12",
    "sails.io.js": "^1.0.0",
    "socket.io-client": "1.7.3",
    "socket.io-redis": "3.1.0",
    "eslint": "3.5.0"
  },
  "scripts": {
    "test": "nodever=`node -e \"console.log('\\`node -v\\`'[1]);\"` && if [ $nodever != \"0\" ]; then npm run lint; fi && npm run custom-tests",
    "custom-tests": "node ./node_modules/mocha/bin/mocha test/** --timeout 5000",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-hook-sockets.git"
  },
  "sails": {
    "isHook": true
  },
  "author": "Mike McNeil",
  "license": "MIT"
}
